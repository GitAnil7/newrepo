pipeline{
    agent any
    stages {
        stage('master branch') {
            steps {
                sh 'echo "this is master branch" '
            }
        }
    stages {
        stage('dev1') {
            steps {
                sh 'echo "sprint1 applicaiton" '
            }
        }
    stages {
        stage('hotfix') {
            steps {
                sh 'echo "deploy application" '
            }
        }
    }
}
}
}
